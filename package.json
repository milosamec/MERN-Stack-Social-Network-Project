  "bugs": {
    "url": "https://github.com/Zolw95/MERN-Stack-Social-Network-Project/issues"
  },
  "homepage": "https://github.com/Zolw95/MERN-Stack-Social-Network-Project#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.9.20",
    "normalize-url": "^5.0.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
